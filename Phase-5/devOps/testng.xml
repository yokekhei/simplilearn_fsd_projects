<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Headless Browser Suite" thread-count="5" parallel="tests">
	<test name="Chrome Testing">
		<parameter name="browserType" value="chrome headless"></parameter>
		<classes>
			<class name="org.yokekhei.fsd.p5.UserLoginTests" />
			<class name="org.yokekhei.fsd.p5.UserRegisterTests" />
			<class name="org.yokekhei.fsd.p5.UserLogoutTests" />
			<class name="org.yokekhei.fsd.p5.UserCourseTests" />
			<class name="org.yokekhei.fsd.p5.AdminLoginTests" />
			<class name="org.yokekhei.fsd.p5.AdminLogoutTests" />
			<class name="org.yokekhei.fsd.p5.AdminCourseTests" />
		</classes>
	</test>
	<test name="Firefox Testing">
		<parameter name="browserType" value="firefox headless"></parameter>
		<classes>
			<class name="org.yokekhei.fsd.p5.UserLoginTests" />
			<class name="org.yokekhei.fsd.p5.UserRegisterTests" />
			<class name="org.yokekhei.fsd.p5.UserLogoutTests" />
			<class name="org.yokekhei.fsd.p5.UserCourseTests" />
			<class name="org.yokekhei.fsd.p5.AdminLoginTests" />
			<class name="org.yokekhei.fsd.p5.AdminLogoutTests" />
			<class name="org.yokekhei.fsd.p5.AdminCourseTests" />
		</classes>
	</test>
</suite>
